page {
  height: 100%;
}
.cate .cates_list {
  height: calc(100vh - 270rpx);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.cate .cates_list .left_menu {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
}
.cate .cates_list .left_menu .menu_item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 80rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 30rpx;
}
.cate .cates_list .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cate .cates_list .right_content {
  -webkit-box-flex: 5;
  -webkit-flex: 5;
          flex: 5;
}
.cate .cates_list .right_content .content_group .group_title {
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.cate .cates_list .right_content .content_group .group_title .delimiter {
  color: gray;
  padding: 0 10rpx;
}
.cate .cates_list .right_content .content_group .group_list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.cate .cates_list .right_content .content_group .group_list navigator {
  width: 33.33%;
  text-align: center;
}
.cate .cates_list .right_content .content_group .group_list navigator image {
  width: 50%;
}

